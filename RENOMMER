<ifmodule mod_rewrite.c>
    Options +FollowSymlinks
    RewriteEngine on
    RewriteRule ^([0-9]+).html /index.php?id=$1 [QSA,L]
</ifmodule>
ErrorDocument 404 http://projetphp/
ErrorDocument 401 http://projetphp/
ErrorDocument 500 http://projetphp/
